{
  "expo": {
    "name": "RestaurantChoos",
    "slug": "RestaurantChoos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/its-decision-time.android.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "backgroundColor": "#6200ea",
    "splash": {
      "image": "./assets/images/its-decision-time.android.png",
      "resizeMode": "contain",
      "backgroundColor": "#6200ea"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kirito22.RestaurantChoos",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/its-decision-time.android.png",
        "backgroundColor": "#6200ea"
      },
      "package": "com.kirito22.RestaurantChoos"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/its-decision-time.android.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/its-decision-time.android.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#6200ea"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6f369256-d3fa-4e9b-83c7-89307f4c6d41"
      }
    }
  }
}
